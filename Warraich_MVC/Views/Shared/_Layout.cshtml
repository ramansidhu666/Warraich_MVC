    <script>
        $(function () {
            debugger;
            var activeurl = window.location.pathname;
            var myString = activeurl.substr(activeurl.indexOf("/") + 6)
            if (myString != "") {
                var latest = $('a[class="' + myString + '"]');
                var prev = $('.navigation li').removeClass('active');
                latest.parent('li').addClass('active');
                if ($(".service").hasClass(myString))
                {
                    $(".service").parent('li').addClass('active');
                }
                if ($(".cooked").hasClass(myString)) {
                    $(".cooked").parent('li').addClass('active');
                }
            }
            else {
                $('a[class="Index"]').parent('li').addClass('active');
